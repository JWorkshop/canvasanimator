{
  "name": "@jworkshop/canvasanimator",
  "version": "0.0.1",
  "description":
    "A canvas react UI component integrated with animation feature.",
  "author": "jworkshop <john.joydesign@gmail.com>",
  "keywords": ["canvas", "animator"],
  "repository": {
    "type": "git",
    "url": "git://github.com/JWorkshop/canvasanimator.git"
  },
  "license": "MIT",
  "dependencies": {
